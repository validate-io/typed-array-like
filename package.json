  "bugs": {
    "url": "https://github.com/validate-io/typed-array-like/issues"
  },
  "dependencies": {
    "compute-const-max-safe-integer": "^1.0.0",
    "validate.io-integer-primitive": "^1.0.0"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "mocha": "2.x.x",
    "coveralls": "^2.11.1",
